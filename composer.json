{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "Behat cross-module integration testing",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe-themes/simple": "^3",
        "silverstripe/login-forms": "^4",
        "silverstripe/recipe-cms": "4.x-dev",
        "silverstripe/recipe-content-blocks": "2.x-dev",
        "silverstripe/framework": "dev-pulls/4/dataobject-usage as 4.x-dev",
        "silverstripe/admin": "dev-pulls/1/usedontable-usage as 1.x-dev",
        "silverstripe/asset-admin": "dev-pulls/1/usedontable-updates as 1.x-dev",
        "silverstripe/versioned-admin": "dev-pulls/1/usedontable-extension as 1.x-dev",
        "dnadesign/silverstripe-elemental": "dev-pulls/4/update-used-on-table-extension as 4.x-dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-elemental.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-versioned-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-asset-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-framework.git"
        }
    ],
    "extra": {
        "project-files": [
            "app/_config/*",
            ".env.example"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ],
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/contentblocks.yml",
            "app/_config/database.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/BlocksPage.php",
            "app/src/Page.php",
            "app/src/PageController.php",
            "behat.yml",
            "phpcs.xml.dist",
            "phpunit.xml.dist"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "platform": {
            "php": "7.1"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
